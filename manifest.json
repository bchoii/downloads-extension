{
  "name": "Downloads by Date",
  "description": "Downloads by Date",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "clipboardWrite",
    "clipboardRead",
    "webRequest",
    "downloads",
    "bookmarks",
    "cookies",
    "declarativeContent",
    "storage",
    "scripting"
  ],
  "background": { "service_worker": "serviceworker.js" },
  "host_permissions": ["*://*/*"],
  "action": {}
}
